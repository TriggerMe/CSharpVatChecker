<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <AssemblyName>TriggerMe.VATChecker</AssemblyName>
  </PropertyGroup>

  <PropertyGroup>
    <IsPackable>true</IsPackable>
    <PackageVersion>0.0.2</PackageVersion>
    <Title>VAT Number Checker</Title>
    <Description>A .NET Standard VAT Checker implementation using the VIES database (http://ec.europa.eu/taxation_customs/vies)</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <ProjectUrl>https://github.com/TriggerMe/CSharpVatChecker</ProjectUrl>
    <RepositoryUrl>https://github.com/TriggerMe/CSharpVatChecker</RepositoryUrl>
    <Authors>jamesikanos</Authors>
  </PropertyGroup>

</Project>
